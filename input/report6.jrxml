	<queryString>
		<![CDATA[SELECT
    tb_region.`str_name_region` AS Região,
    SUM(tb_payments.`db_value`) AS Valor_Total
FROM
    db_eca.`tb_city` tb_city
        INNER JOIN
    db_eca.`tb_payments` tb_payments ON db_eca.tb_city.`id_city` = db_eca.tb_payments.`tb_city_id_city`
        INNER JOIN
    db_eca.`tb_state` tb_state ON db_eca.tb_city.`tb_state_id_state` = db_eca.tb_state.`id_state`
        INNER JOIN
    db_eca.`tb_region` tb_region ON db_eca.tb_state.`tb_region_id_region` = db_eca.tb_region.`id_region`
GROUP BY tb_region.`str_name_region`
ORDER BY db_eca.tb_region.`str_name_region` ASC;]]>
	</queryString>
